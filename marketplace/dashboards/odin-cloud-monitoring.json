{
  "dashboardFilters": [],
  "displayName": "ODIN Cloud Monitoring",
  "mosaicLayout": {
    "columns": 48,
    "tiles": [
      { "xPos": 0, "yPos": 0, "width": 24, "height": 8,
        "widget": { "title": "Gateway HTTP p95 latency", "xyChart": { "dataSets": [ { "timeSeriesQuery": { "timeSeriesFilter": { "filter": "metric.type=\"external.googleapis.com/prometheus/http_request_duration_seconds\" AND metric.label.service=\"odin-gateway\"", "secondaryAggregation": { "perSeriesAligner": "ALIGN_PERCENTILE_95" } } } } ] } } },
      { "xPos": 24, "yPos": 0, "width": 24, "height": 8,
        "widget": { "title": "Relay HTTP p95 latency", "xyChart": { "dataSets": [ { "timeSeriesQuery": { "timeSeriesFilter": { "filter": "metric.type=\"external.googleapis.com/prometheus/http_request_duration_seconds\" AND metric.label.service=\"odin-relay\"", "secondaryAggregation": { "perSeriesAligner": "ALIGN_PERCENTILE_95" } } } } ] } } },
      { "xPos": 0, "yPos": 8, "width": 24, "height": 8,
        "widget": { "title": "ODIN hops emitted", "xyChart": { "dataSets": [ { "timeSeriesQuery": { "timeSeriesFilter": { "filter": "metric.type=\"external.googleapis.com/prometheus/odin_hops_total\"" } } } ] } } },
      { "xPos": 24, "yPos": 8, "width": 24, "height": 8,
        "widget": { "title": "HTTP 5xx rate", "xyChart": { "dataSets": [ { "timeSeriesQuery": { "timeSeriesFilter": { "filter": "metric.type=\"external.googleapis.com/prometheus/http_requests_total\" AND metric.label.code=~\"5..\"" } } } ] } } }
    ]
  }
}
